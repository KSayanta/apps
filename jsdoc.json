{
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": ["true"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "tutorials": "./tutorials"
  }
}
